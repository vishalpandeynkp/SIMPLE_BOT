  "repository": "https://github.com/vishalpandeynkp/SIMPLE_BOT",
  "env": {
    "API_ID": {
      "description": "Your Telegram API ID",
      "required": true
    },
    "API_HASH": {
      "description": "Your Telegram API Hash",
